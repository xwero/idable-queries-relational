{
  "name": "xwero/idable-queries-relational",
  "description": "Idable queries package for relational databases",
  "type": "library",
  "license": "mit",
  "version": "0.3.0",
  "minimum-stability": "dev",
  "require": {
    "php": ">=8.4",
    "ext-pdo": "*",
    "xwero/idable-queries-core": "^0.2.0"
  },
  "autoload": {
    "psr-4": {
      "Xwero\\IdableQueriesRelational\\": "src/"
    },
    "files": [
      "src/functions.php"
    ]
  },
  "require-dev": {
    "pestphp/pest": "4.x-dev"
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\" : "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  },
  "scripts": {
    "tests": "XDEBUG_MODE=coverage ./vendor/bin/pest"
  }
}
